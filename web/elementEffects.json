{
  "options|autoSelectCopiesToClipboard": {
    "default": [
      {
        "id": "tabSwitchCopiesToClipboard",
        "selector": "input",
        "property": "disabled",
        "value": true
      },
      {
        "id": "autoCompleteCopiesToClipboard",
        "selector": "input",
        "property": "disabled",
        "value": true
      },
      {
        "id": "afterURLBarFocus",
        "selector": "strong",
        "property": "textContent",
        "value": "but does not copy to clipboard (i.e. a subsequent middle-click paste will not paste the URL)"
      },
      {
        "id": "afterURLBarFocus",
        "selector": "img",
        "property": "alt",
        "value": "URL bar is focused; everything is selected, but nothing copied to the selection clipboard."
      },
      {
        "id": "afterSearchBarFocus",
        "selector": "strong",
        "property": "textContent",
        "value": "but does not copy to clipboard"
      },
      {
        "id": "afterSearchBarFocus",
        "selector": "img",
        "property": "alt",
        "value": "Search bar is focused; everything is selected, but nothing copied to the selection clipboard."
      },
      {
        "id": "afterTabSwitch",
        "selector": "strong",
        "property": "textContent",
        "value": "but its content is not copied to clipboard"
      },
      {
        "id": "afterTabSwitch",
        "selector": "img:last-of-type",
        "property": "alt",
        "value": "A tab is shown; URL bar is selected, but nothing copied to the selection clipboard."
      },
      {
        "id": "afterURLBarAutoComplete",
        "selector": "strong",
        "property": "textContent",
        "value": "but the selection is not copied to clipboard"
      },
      {
        "id": "afterURLBarAutoComplete",
        "selector": "img:last-of-type",
        "property": "alt",
        "value": "The start of a URL is typed; the rest of the URL is selected, but nothing copied to the selection clipboard."
      }
    ],
    "otherwise": [
      {
        "id": "tabSwitchCopiesToClipboard",
        "selector": "input",
        "property": "disabled",
        "value": false
      },
      {
        "id": "autoCompleteCopiesToClipboard",
        "selector": "input",
        "property": "disabled",
        "value": false
      },
      {
        "id": "afterURLBarFocus",
        "selector": "strong",
        "property": "textContent",
        "value": "and copies to clipboard (i.e. a subsequent middle-click paste will paste the URL)"
      },
      {
        "id": "afterURLBarFocus",
        "selector": "img",
        "property": "alt",
        "value": "URL bar is focused; everything is selected, and the selection is copied to the selection clipboard."
      },
      {
        "id": "afterSearchBarFocus",
        "selector": "strong",
        "property": "textContent",
        "value": "and copies to clipboard"
      },
      {
        "id": "afterSearchBarFocus",
        "selector": "img",
        "property": "alt",
        "value": "Search bar is focused; everything is selected, and the selection is copied to the selection clipboard."
      }
    ]
  },
  "options|clearSearchBarOnSubmit": {
    "default": [
      {
        "id": "afterSubmittingSearch",
        "selector": "strong",
        "property": "textContent",
        "value": "does not clear the search bar"
      },
      {
        "id": "afterSubmittingSearch",
        "selector": "img:last-of-type",
        "property": "src",
        "value": "web/img/Search%20bar%20not%20selected,%20suggestions%20closed.png"
      },
      {
        "id": "afterSubmittingSearch",
        "selector": "img:last-of-type",
        "property": "alt",
        "value": "Search bar is focused, the previously typed query remains."
      }
    ],
    "otherwise": [
      {
        "id": "afterSubmittingSearch",
        "selector": "strong",
        "property": "textContent",
        "value": "clears the search bar"
      },
      {
        "id": "afterSubmittingSearch",
        "selector": "img:last-of-type",
        "property": "src",
        "value": "web/img/Search%20bar%20empty.png"
      },
      {
        "id": "afterSubmittingSearch",
        "selector": "img:last-of-type",
        "property": "alt",
        "value": "Search bar is focused, and empty; the previously typed query has been cleared."
      }
    ]
  },
  "options|doubleClickSelectsAll": {
    "default": [
      {
        "id": "afterDoubleClick",
        "selector": "strong",
        "property": "textContent",
        "value": "a single word is selected"
      },
      {
        "id": "afterDoubleClick",
        "selector": "img",
        "property": "src",
        "value": "web/img/Word%20selected.png"
      },
      {
        "id": "afterDoubleClick",
        "selector": "img",
        "property": "alt",
        "value": "URL bar is focused; the double-clicked word is selected."
      }
    ],
    "otherwise": [
      {
        "id": "afterDoubleClick",
        "selector": "strong",
        "property": "textContent",
        "value": "everything is selected"
      },
      {
        "id": "afterDoubleClick",
        "selector": "img",
        "property": "src",
        "value": "web/img/All%20selected.png"
      },
      {
        "id": "afterDoubleClick",
        "selector": "img",
        "property": "alt",
        "value": "URL bar is focused; everything is selected."
      }
    ]
  },
  "options|preventClickSelectsAll": {
    "default": [
      {
        "id": "afterSingleClick",
        "selector": "strong",
        "property": "textContent",
        "value": "the URL bar is focused, but not selected"
      },
      {
        "id": "afterSingleClick",
        "selector": "img",
        "property": "src",
        "value": "web/img/Focused.png"
      },
      {
        "id": "afterSingleClick",
        "selector": "img",
        "property": "alt",
        "value": "URL bar is focused; nothing is selected."
      }
    ],
    "otherwise": [
      {
        "id": "afterSingleClick",
        "selector": "strong",
        "property": "textContent",
        "value": "everything is selected"
      },
      {
        "id": "afterSingleClick",
        "selector": "img",
        "property": "src",
        "value": "web/img/All%20selected.png"
      },
      {
        "id": "afterSingleClick",
        "selector": "img",
        "property": "alt",
        "value": "URL bar is focused; everything is selected."
      }
    ]
  },
  "options|secondsSeekedByKeyboard": {
    "default": [
      {
        "id": "afterSeekInBuiltInPlayer",
        "selector": "strong",
        "property": "textContent",
        "value": "the default amount of seconds"
      },
      {
        "id": "afterSeekInPiPPlayer",
        "selector": "strong",
        "property": "textContent",
        "value": "the default amount of seconds"
      }
    ]
  }
}
